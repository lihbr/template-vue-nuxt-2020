{
  "name": "template-vue-nuxt",
  "version": "2.4.0",
  "description": "A Nuxt.js starter pack to build app with Vue.js",
  "author": "lihbr",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern src/modules/*/*.plugin.js ."
  },
  "engines": {
    "node": ">=10.16.3",
    "yarn": ">=1.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "color": "3.1.2",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "is-mobile": "2.1.0",
    "marked": "0.7.0",
    "mouse-wheel": "1.2.0",
    "netlify-cms": "2.9.7",
    "nuxt": "2.10.1",
    "object-fit-images": "3.2.4",
    "prismjs": "1.17.1",
    "scrollmagic": "2.0.7",
    "stats-js": "1.0.1",
    "tailwindcss": "1.1.2",
    "tailwindcss-transitions": "2.1.0",
    "vue-functional-data-merge": "3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@nuxt/common": "2.3.4",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/netlify-files": "1.0.0",
    "@nuxtjs/sitemap": "2.0.0",
    "@nuxtjs/style-resources": "1.0.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-vue": "5.2.3",
    "fibers": "4.0.1",
    "globby": "10.0.1",
    "husky": "3.0.9",
    "nodemon": "1.19.4",
    "nuxt-svg-loader": "1.0.1",
    "prettier": "1.18.2",
    "sass": "1.23.0",
    "sass-loader": "7.1.0",
    "slugify": "1.3.5"
  }
}
